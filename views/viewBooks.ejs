                    <div class="content">
                        <div class="container">
                            <a href="/users/books/pdfview"><button id="genReportBtn" class="btn" style="margin-left: 920px;">Generate PDF</button></a>
                            
                            <h2 class="mb-5">Books <b> Record</h2>
                            <div class="table-responsive">
                                <table class="table custom-table">
                                    <thead>
                                        <tr>
                                            <th scope="col">
                                                <label class="form-check-label">
                                      <input type="checkbox" class="js-check-all"/>
                                      
                                    </label>
                                            </th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Author</th>
                                            <th scope="col">Publish Date</th>
                                            <th scope="col">ISBN</th>
                                            <th scope="col">Page Count</th>
                                            <th scope="col">Word Count</th>
                                            <th scope="col">Rating</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% records.forEach(function(row){ %>
                                        <tr>
                                            <th scope="row">
                                                <label class="form-check-label">
                                                    <input type="checkbox"/>
                                                </label>
                                            </th>
                                            <td><%= row.name %></td>
                                            <td><%= row.author %></td>
                                            <td><%= row.publishDate %></td>
                                            <td><%= row.isbn %></td>
                                            <td><%= row.pageCount %></td>
                                            <td><%= row.wordCount %></td>
                                            <td><%= row.rating %></td>
                                        </tr>
                                        <% })%>
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
